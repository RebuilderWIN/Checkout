<h1>Asset Tags</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Uid</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @asset_tags.each do |asset_tag| %>
      <tr>
        <td><%= asset_tag.id %></td>
        <td><%= link_to asset_tag.uid, asset_tag_path(asset_tag) %></td>
        <td><%= asset_tag.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_asset_tag_path(asset_tag), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', asset_tag_path(asset_tag), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_asset_tag_path, :class => 'btn btn-primary' %>
