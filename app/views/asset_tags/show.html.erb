<h1>Asset Tag</h1>

<p>
  <b>Uid</b><br>
  <%= @asset_tag.uid %>
</p>

<p>
  <b>Part</b><br>
  <%= @asset_tag.part_id %>
</p>

<div class="form-actions">
  <%= link_to 'Back', asset_tags_path, :class => 'btn'  %>
  <%= link_to 'Edit', edit_asset_tag_path(@asset_tag), :class => 'btn' %>
  <%= link_to 'Delete', asset_tag_path(@asset_tag), :method => 'delete', :confirm => 'Are you sure?', :class => 'btn btn-danger' %>
</div>
